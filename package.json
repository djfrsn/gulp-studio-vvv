{
  "name": "gulp-studio",
  "version": "0.2.0",
  "description": "Repository of modern web development tooling and components",
  "repository": {
    "type": "git",
    "url": "git://github.com/djfrsn/studio.git"
  },
  "browser": {
    "underscore": "backbone/node_modules/underscore",
    "plugin": "./src/javascript/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "^1.3.2",
    "browserify": "~3.36.0",
    "browserify-shim": "~3.4.1",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "0.0.8",
    "gulp-changed": "^0.4.1",
    "gulp-concat": "^2.3.4",
    "gulp-csscomb": "^3.0.2",
    "gulp-filesize": "0.0.6",
    "gulp-filter": "^1.0.0",
    "gulp-imagemin": "~0.1.5",
    "gulp-jshint": "^1.8.0",
    "gulp-minify-css": "^0.3.7",
    "gulp-notify": "~1.2.4",
    "gulp-plumber": "^0.6.4",
    "gulp-remember": "^0.2.0",
    "gulp-rename": "^1.2.0",
    "gulp-ruby-sass": "^0.7.0",
    "gulp-shell": "^0.2.9",
    "gulp-uglify": "^0.3.1",
    "gulp-util": "~2.2.14",
    "hbsfy": "~1.3.2",
    "jshint-stylish": "^0.4.0",
    "rimraf": "^2.2.8",
    "run-sequence": "^0.3.6"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.1"
  }
}
